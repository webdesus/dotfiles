partial
xkb_compatibility "super_mappings" {
    interpret KP_Left {
        repeat= True;
        action= RedirectKey(keycode=<LEFT>, clearmods=Alt);
    };
    interpret KP_Right {
        repeat= True;
        action= RedirectKey(keycode=<RGHT>, clearmods=Alt);
    };
    interpret KP_Up {
        repeat= True;
        action= RedirectKey(keycode=<UP>, clearmods=Alt);
    };
    interpret KP_Down {
