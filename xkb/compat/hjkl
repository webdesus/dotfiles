    };
    interpret KP_Next {
        repeat= True;
        action= RedirectKey(keycode=<PGDN>, clearmods=Alt);
    };
    interpret KP_Home {
        repeat= True;
        action= RedirectKey(keycode=<HOME>, clearmods=Alt);
    };
    interpret KP_End {
        repeat= True;
        action= RedirectKey(keycode=<END>, clearmods=Alt);
    };
    interpret KP_Enter {
        repeat= True;
        action= RedirectKey(keycode=<RTRN>, clearmods=Alt);
    };
    interpret KP_Add {
        repeat= True;
        action= RedirectKey(keycode=<BKSP>, clearmods=Alt);
    };
    interpret KP_Delete {
        repeat= True;
        action= RedirectKey(keycode=<DELE>, clearmods=Alt);
    };
    interpret KP_1 {
        repeat= True;
        action= RedirectKey(keycode=<AE01>, clearmods=Mod3);
    };
    interpret KP_2 {
        repeat= True;
        action= RedirectKey(keycode=<AE02>, clearmods=Mod3);
    };
    interpret KP_3 {
        repeat= True;
